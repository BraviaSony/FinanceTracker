{
  "name": "@rebolt-ai/convex",
  "version": "0.1.10",
  "description": "Rebolt AI Convex SDK",
  "files": [
    "dist"
  ],
  "exports": {
    "./v1": {
      "types": "./dist/convex/src/v1/index.d.ts",
      "import": "./dist/v1/index.mjs",
      "require": "./dist/v1/index.js"
    },
    "./v1/*": {
      "types": "./dist/convex/src/v1/*/index.d.ts",
      "import": "./dist/v1/*/index.mjs",
      "require": "./dist/v1/*/index.js"
    }
  },
  "peerDependencies": {
    "convex": "^1.25.4",
    "convex-helpers": "^0.1.100"
  },
  "peerDependenciesMeta": {
    "convex": {
      "optional": false
    },
    "convex-helpers": {
      "optional": false
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/node": "^24.0.4",
    "commander": "^12.1.0",
    "jsdom": "^23.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "@types/express": "^4.17.23",
    "dotenv": "^16.4.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "scripts": {
    "build": "tsup",
    "dev": "dotenv tsup --watch",
    "lint": "biome lint ./src",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "biome format ./src --write",
    "format:check": "biome format ./src",
    "types": "tsc --emitDeclarationOnly"
  }
}